{
    "restartable": "rs",
    "ignore": [
        ".git",
        "Gruntfile.js",
        "bower_components/**/*",
        "node_modules/**/*",
        "src/**/*",
        "_site/**/*"
    ],
    "verbose": true,
    "execMap": {
        "js": "node --harmony"
    },
    "env": {
        "NODE_ENV": "development"
    },
    "ext": "js json"
}